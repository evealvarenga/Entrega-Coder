{{#if id}}
<h2>Lista de productos</h2>
    {{#each products}}
    <li>
        {{thumbnail}}
        <h3>{{title}}</h3>
        <p><i>Precio ${{price}}</i> - Unidades disponibles: {{stock}}</p>
        <p>{{description}}</p>
    </li>
    {{/each}}
{{#else}}
    <p>No se encontraron productos</p>
{{/if}}